[setup]

source = repository
vcs = git


[vcs]

enable_plugins = git


[renderer]

enable_plugins = htmlbox text


[renderer.htmlbox]

destfile = index.html
destdir = html
href_prefix = html/
templatedirs = templates
template = page.html
staticdir = static


[renderer.htmlbox.target]

enable_plugins = image


[action]

enable_plugins = latex
