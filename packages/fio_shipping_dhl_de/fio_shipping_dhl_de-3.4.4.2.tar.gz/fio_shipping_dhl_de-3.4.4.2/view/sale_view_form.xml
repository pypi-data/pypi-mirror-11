<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id=&quot;shipments&quot;]" position="after">
        <page string="DHL DE" id="dhl_de" states="{'invisible': Not(Bool(Eval('is_dhl_de_shipping')))}">
            <label name="dhl_de_product_code"/>
            <field name="dhl_de_product_code"/>
            <newline/>
            <group colspan="4" col="6" id="dhl_de_international"
			string="International Shipping" states="{'invisible': Not(Bool(Eval('is_international_shipping')))}">
                <label name="dhl_de_export_type"/>
                <field name="dhl_de_export_type"/>
                <label name="dhl_de_terms_of_trade"/>
                <field name="dhl_de_terms_of_trade"/>
                <label name="dhl_de_export_type_description"/>
                <field name="dhl_de_export_type_description"/>
            </group>
        </page>
    </xpath>
</data>
