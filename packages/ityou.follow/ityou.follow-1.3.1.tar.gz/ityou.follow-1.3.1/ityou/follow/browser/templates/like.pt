<tal:is_like_enabled 
    tal:define="template_id context/plone_context_state/view_template_id"
    tal:condition="view/likeEnabled">

    <div id="like-viewlet" class="like"  i18n:domain="ityou.follow" 
         tal:condition="python:not view.template_blacklist(template_id)">

        <div id="sid" tal:define="data-ityou-sid python:request.get('SESSION').id;" />
        <!-- voting -->     
        <div  id="voting-uid" class="voting-container"
              tal:attributes="data-ityou-uid context/UID;
                              data-ityou-sid python: view.get_auth_user() or request.get('SESSION').id;" >
            <div class="voting-flop">
                <i class="fa fa-thumbs-o-down"/><span class="vote-flop">0</span>
            </div>
            <div class="voting-top">
                <i class="fa fa-thumbs-o-up"/><span class="vote-top">0</span> 
            </div>
        </div>
        <!-- /voting -->

    </div>
</tal:is_like_enabled>
