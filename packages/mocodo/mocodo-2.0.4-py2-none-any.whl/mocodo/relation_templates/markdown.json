{
  "extension": ".md",
  "compose_primary_key": "<u>{label}</u>",
  "compose_foreign_key": "_{label}_",
  "compose_foreign_primary_key": "<u>_{label}_</u>",
  "compose_relation": "**{this_relation_name}** ({columns})",
  "transform_single_column_relation": [
    {
      "search": "(?s)(.+)",
      "replace": "<!---\n\\1\n--->"
    }
  ],
  "relation_separator": "  \n"
}