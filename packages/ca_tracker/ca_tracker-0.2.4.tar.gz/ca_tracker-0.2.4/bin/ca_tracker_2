#!/usr/bin/env python

import ca_tracker
print 'starting merge_tracking_data'

exp_filename = '50ms_7p5_1'
ca_tracker.merge_tracking_data(exp_filename, ca_tracker.config)
print 'starting compute features'
ca_tracker.compute_features(exp_filename, ca_tracker.config)
print 'starting classify cells'
ca_tracker.classify_cells(exp_filename, ca_tracker.config)
print 'starting intensity extraction'
ca_tracker.extract_intensity(exp_filename, ca_tracker.config)
print 'starting plot profiles'
ca_tracker.plot_profiles(exp_filename, ca_tracker.config)