                {
                    "type": "string",
                    "name": "All Ply",
                    "description": "Everything is made out of ply",
                    "value": "all_ply",
                    "#top_finish": "/finishes/opendesk/premium-birch-ply"

                },
                {
                    "type": "string",
                    "name": "Wisa Top",
                    "description": "The top is made from wisa and the legs from ply",
                    "value": "wisa_top",
                    "#top_finish": "/finishes/opendesk/premium-wisa"

                }
            ]
        }
