% for domain in domains:
FromOrTo:	*@${domain.name}	%reports-base%/${domain.language}/stored.filename.message.txt
% for alias in domain.aliases:
% if alias.status:
FromOrTo:	*@${alias.name}	%reports-base%/${domain.language}/stored.filename.message.txt
% endif
% endfor
% endfor
# Default rule
FromOrTo:  default  %report-dir%/stored.filename.message.txt
