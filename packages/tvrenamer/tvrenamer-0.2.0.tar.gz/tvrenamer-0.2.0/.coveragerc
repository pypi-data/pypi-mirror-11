[run]
branch = True
source = tvrenamer
omit = tvrenamer/tests/*, tvrenamer/common/encodeutils.py, tvrenamer/common/_service.py

[report]
ignore-errors = True

exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError


