{
  "base_url": "https://api-dev.quanttus.com",
  "port": 80,

   "logger": {
     "version": 1,
     "disable_existing_loggers": false,
     "formatters": {
       "simple": {
         "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
       }
     },

     "handlers": {
       "console": {
         "class": "logging.StreamHandler",
         "level": "DEBUG",
         "formatter": "simple",
         "stream": "ext://sys.stdout"
       },

       "file_handler": {
         "class": "logging.handlers.RotatingFileHandler",
         "formatter": "simple",
         "filename": "quanttus_upload.log",
         "maxBytes": 10485760,
         "backupCount": 20,
         "encoding": "utf8"
       }
     },

     "root": {
       "level": "INFO",
       "handlers": ["console"]
     }
   }
}
