msgstr "Crear una cuenta"

#: templates/registration/login.html:13
msgid "Sign in using Facebook or Twitter"
msgstr "Entrar utilizando Facebook o Twitter"

#: templates/registration/login.html:18
msgid "You can use your Facebook or Twitter accounts to log in"
msgstr "Puedes utilizar tu cuenta de Twitter o Facebook para entrar"

#: templates/registration/login.html:29
#: templates/registration/registration_form.html:7
msgid "You are already logged in"
msgstr "Ya estás dentro"

#: templates/registration/login.html:35
