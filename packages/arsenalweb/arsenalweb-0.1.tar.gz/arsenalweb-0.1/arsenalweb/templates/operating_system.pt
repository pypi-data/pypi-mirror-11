<link href="/static/css/singleton.css" rel="stylesheet" type="text/css" />

<div tal:condition="operating_system">
  <div id="page_title_s">
    <span class="page_title_type_s">objects/operating_system/</span> <span class="page_title_name_s">${operating_system.variant} ${operating_system.version_number}</span>
  </div>

  <ul class="tabs">
    <li>
      <input type="radio" checked name="tabs" id="tab1">
      <label for="tab1">Operating System</label>
      <div id="tab-content1" class="tab-content">
        Operating System Name: <span class="mono">${operating_system.variant} ${operating_system.version_number}</span><br>
        Operating System ID: <span class="mono">${operating_system.operating_system_id}</span><br>
        Variant: <span class="mono">${operating_system.variant}</span><br>
        Version Number: <span class="mono">${operating_system.version_number}</span><br>
        Architecture: <span class="mono">${operating_system.architecture}</span><br>
        Description: <span class="mono">${operating_system.description}</span><br>
        Created At: <span class="mono">${operating_system.created}</span><br>
        Updated At: <span class="mono">${operating_system.updated}</span><br>
        Updated By: <span class="mono">${operating_system.updated_by}</span><br>
      </div>
    </li>
  </ul>
</div>

<div tal:condition="not operating_system">
  <div id="page_title_s">
    <span class="page_title_type_s">objects/operating_system/</span> <span class="page_title_name_s">Not Found</span>
  </div>
</div>
